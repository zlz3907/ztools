<ivysettings>
	<settings defaultResolver="co" />
	<caches default="l-cache" resolutionCacheDir="${basedir}/ivy/cache">
		<cache name="l-cache" basedir="${basedir}/ivy/cache" />
	</caches>
	<resolvers>
		<chain name="co" returnFirst="true">
			<filesystem name="co-local">
				<artifact pattern="/home/lizhi/ivyrepository/[artifact]-[revision].[ext]" />
			</filesystem>
			<ibiblio name="ibiblio" />
		</chain>
	</resolvers>
</ivysettings>